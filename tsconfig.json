{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "outDir": "./dist",
      "allowJs": true,
      "checkJs": false,
      "noEmit": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "suppressImplicitAnyIndexErrors": false
    },
    "include": ["./**/*.ts", "./**/*.js"],
    "exclude": ["node_modules"]
  }
  